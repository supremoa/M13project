<resources>

    <!-- Base application theme. -->

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#013c81</item>
        <item name="colorPrimaryDark">#013c81</item>

        <!-- <item name="colorAccent">@color/mama_natura</item>-->
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="frame" parent="android:style/Theme.Dialog">
        <item name="android:background">@color/walafondo</item>
        <item name="android:textColor">#000</item>
      <!---  <item name="android:windowNoTitle">true</item>-->
    </style>

    <style name="AppTheme.NoActionBar.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.NoActionBar.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
